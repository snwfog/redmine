var clickHandler = function(evt)
{
  console.log("click handler");
};


var $span = $("<%= j("#favorite_project#{@project.id}") %>");
var $parentTr = $(this).parents('tr');
if ("<%= favorite?(@project) %>" == "true" && $parentTr.hasClass('unfav'))
{
  $parentTr.removeClass('unfav').addClass('fav');
}
else if ($parentTr.hasClass('fav'))
{
  $parentTr.removeClass('fav').addClass('unfav');
}

var $newSpan = $("<%= j favorite_tag(@project, User.current) %>");
$span.find('a').on('click', clickHandler);
$span.replaceWith($newSpan);

<% @project = nil %>
$('#quick-search > select').html("<%= j render_project_jump_box %>");

